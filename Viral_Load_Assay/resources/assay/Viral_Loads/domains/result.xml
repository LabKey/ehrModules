    <exp:PropertyDescriptor>
        <exp:Name>eluateVol</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#double</exp:RangeURI>
        <exp:Label>Eluate Volume</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>dilutionFactor</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#double</exp:RangeURI>
        <exp:Label>Dilution Factor</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>volPerRxn</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#double</exp:RangeURI>
        <exp:Label>Sample Vol Per Rxn</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>cp</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#double</exp:RangeURI>
