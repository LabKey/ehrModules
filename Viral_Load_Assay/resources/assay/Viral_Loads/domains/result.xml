<ap:domain xmlns:exp="http://cpas.fhcrc.org/exp/xml" xmlns:ap="http://labkey.org/study/assay/xml">
    <exp:Description>These are the viral load result fields.</exp:Description>
    <exp:PropertyDescriptor>
        <exp:Name>subjectId</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:ConceptURI>http://cpas.labkey.com/Study#ParticipantId</exp:ConceptURI>
        <exp:Label>Subject Id</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>sampleId</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#int</exp:RangeURI>
        <exp:Label>Freezer Id</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>date</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#dateTime</exp:RangeURI>
        <exp:ConceptURI>http://cpas.labkey.com/laboratory#sampleDate</exp:ConceptURI>
        <exp:Label>Sample Date</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>sampleType</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Sample Type</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>category</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Category</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>assayId</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Assay Id</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>copiesPerRxn</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#double</exp:RangeURI>
        <exp:Label>Copies Per Rxn</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>sourceMaterial</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
