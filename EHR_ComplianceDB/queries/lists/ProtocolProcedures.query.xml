<query xmlns="http://labkey.org/data/xml/query">
<metadata>
    <tables xmlns="http://labkey.org/data/xml">
        <table tableName="ProtocolProcedures" tableDbType="NOT_IN_DB">
            <columns>
                <column columnName="protocol">
                    <fk>
                        <fkFolderPath>/WNPRC/EHR/</fkFolderPath>
                        <fkDbSchema>lists</fkDbSchema>
                        <fkTable>protocol</fkTable>
                        <fkColumnName>protocol</fkColumnName>
                    </fk>
                </column>
                <column columnName="snomed">
                    <datatype>varchar</datatype>
                    <columnTitle>SNOMED Code</columnTitle>
                    <fk>
                        <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                        <fkDbSchema>ehr_lookups</fkDbSchema>
                        <fkTable>snomed</fkTable>
                        <fkColumnName>code</fkColumnName>
                    </fk>
                </column>
                <column columnName="procedure_name">
                    <columnTitle>Procedure Name</columnTitle>
                </column>
                <column columnName="allowed">
                    <columnTitle># Allowed</columnTitle>
                </column>
            </columns>
            <pkColumnName>rowid</pkColumnName>
        </table>
    </tables>
</metadata>
</query>
