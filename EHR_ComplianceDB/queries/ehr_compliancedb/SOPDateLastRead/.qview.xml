<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="EmployeeId"/>
        <column name="SOPID"/>
        <column name="sopid/Name">
            <properties>
                <property name="columnTitle" value="Name"/>
            </properties>
        </column>
        <column name="sopid/PDF">
            <properties>
                <property name="columnTitle" value="PDF"/>
            </properties>
        </column>
        <column name="sopid/Spanish_PDF">
            <properties>
                <property name="columnTitle" value="Translated PDF"/>
            </properties>
        </column>
        <column name="LastRead"/>
    </columns>
    <sorts>
        <sort column="EmployeeId" descending="false"/>
        <sort column="SOPID" descending="false"/>
    </sorts>
</customView>