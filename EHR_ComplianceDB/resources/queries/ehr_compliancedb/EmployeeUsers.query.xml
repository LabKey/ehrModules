<query xmlns="http://labkey.org/data/xml/query">
<metadata>
    <tables xmlns="http://labkey.org/data/xml">
        <table tableName="EmployeeUsers" tableDbType="NOT_IN_DB">
            <columns>
                <column columnName="employeeId">
                      <isHidden>true</isHidden>
                </column>
                <column columnName="BothCorrect">
                    <isHidden>true</isHidden>
                </column>
                <column columnName="HasUsername">
                    <columnTitle>Has A LabKey Username?</columnTitle>
                </column>
                <column columnName="EmailExists">
                    <columnTitle>Email Matches Their LabKey Username?</columnTitle>
                </column>
                <column columnName="SOPAccess">
                    <columnTitle>Has SOP Access?</columnTitle>
                </column>
                <column columnName="ColonyAccess">
                    <columnTitle>Has Colony Records Access?</columnTitle>
                </column>
            </columns>
        </table>
    </tables>
</metadata>
</query>
