<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="EmployeeId"/>
        <column name="SOP_ID"/>
        <column name="sop_id/Name">
            <properties>
                <property name="columnTitle" value="Name"/>
            </properties>
        </column>
        <column name="sop_id/PDF">
            <properties>
                <property name="columnTitle" value="PDF"/>
            </properties>
        </column>
        <column name="sop_id/Spanish_PDF">
            <properties>
                <property name="columnTitle" value="Translated PDF"/>
            </properties>
        </column>
        <column name="sop_id/video">
        	<properties>
        		<property name="columnTitle" value="Training Video"/>
        	</properties>
        </column>
        <column name="LastRead"/>
    </columns>
    <sorts>
        <sort column="SOP_ID" descending="false"/>
    </sorts>
</customView>