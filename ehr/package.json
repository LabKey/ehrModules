{
  "name": "ehr",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "setup": "npm ci --legacy-peer-deps",
    "build": "npm run build-dev",
    "build-dev": "npm run clean && cross-env NODE_ENV=development webpack --config node_modules/@labkey/build/webpack/dev.config.js --color",
    "build-prod": "npm run clean && cross-env NODE_ENV=production webpack --config node_modules/@labkey/build/webpack/prod.config.js --color --progress --profile",
    "clean": "rimraf resources/web/ehr/gen && rimraf resources/views/gen && rimraf resources/web/gen",
    "start": "cross-env NODE_ENV=development webpack serve --config node_modules/@labkey/build/webpack/watch.config.js",
    "start-link": "cross-env LINK=true npm run start",
    "test": "cross-env NODE_ENV=test jest",
    "lint": "eslint",
    "lint-fix": "eslint --fix",
    "lint-precommit": "node lint.diff.mjs",
    "lint-precommit-fix": "node lint.diff.mjs --fix",
    "lint-branch": "node lint.diff.mjs --currentBranch",
    "lint-branch-fix": "node lint.diff.mjs --currentBranch --fix"
  },
  "jest": {
    "globals": {
      "LABKEY": {
        "container": {
          "formats": {
            "dateFormat": "yyyy-MM-dd",
            "dateTimeFormat": "yyyy-MM-dd HH:mm"
          }
        },
        "moduleContext": {},
        "user": {
          "displayName": "Bill",
          "id": 1004
        }
      }
    },
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "js"
    ],
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "./test/js/setup.ts"
    ],
    "testEnvironment": "jsdom",
    "testMatch": null,
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "testResultsProcessor": "jest-teamcity-reporter",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "@labkey/assayreport": "0.1.3",
    "@labkey/components": "2.194.8",
    "@labkey/eln": "1.5.4",
    "@labkey/freezermanager": "2.27.3",
    "@labkey/workflow": "1.44.6",
    "history": "3.2.1",
    "react-lottie-player": "1.3.3",
    "react-redux": "5.1.0",
    "react-router": "3.2.6",
    "react-router-redux": "4.0.8",
    "redux": "3.7.2",
    "redux-actions": "2.3.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@labkey/build": "6.1.3",
    "@labkey/eslint-config-react": "0.0.11",
    "@labkey/test": "1.1.1",
    "@types/enzyme": "3.10.9",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/history": "3.2.3",
    "@types/jest": "27.4.0",
    "@types/react-redux": "6.0.9",
    "@types/react-router": "3.0.24",
    "@types/react-router-redux": "4.0.49",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "execa": "6.1.0",
    "jest": "27.5.1",
    "jest-teamcity-reporter": "0.9.0",
    "react-test-renderer": "16.14.0",
    "redux-mock-store": "1.5.4",
    "ts-jest": "27.1.4",
    "xhr-mock": "2.5.1"
  }
}
