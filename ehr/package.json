{
  "name": "ehr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "npm ci --legacy-peer-deps",
    "build": "npm run build-dev",
    "build-dev": "npm run clean && cross-env NODE_ENV=development webpack --config node_modules/@labkey/build/webpack/dev.config.js --color",
    "build-prod": "npm run clean && cross-env NODE_ENV=production webpack --config node_modules/@labkey/build/webpack/prod.config.js --color --progress --profile",
    "clean": "rimraf resources/web/ehr/gen && rimraf resources/views/gen && rimraf resources/web/gen",
    "start": "cross-env NODE_ENV=development webpack serve --config node_modules/@labkey/build/webpack/watch.config.js",
    "start-link": "cross-env LINK=true npm run start",
    "test": "cross-env NODE_ENV=test jest",
    "lint-fix": "eslint --fix"
  },
  "dependencies": {
    "@labkey/components": "2.234.0",
    "@labkey/eln": "1.14.0",
    "@labkey/freezermanager": "2.31.0",
    "@labkey/workflow": "1.58.0",
    "history": "3.2.1",
    "react-redux": "5.1.0",
    "react-router": "3.2.6",
    "react-router-redux": "4.0.8",
    "redux": "3.7.2",
    "redux-actions": "2.3.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@labkey/build": "6.3.2",
    "@labkey/eslint-config-react": "0.0.11",
    "@types/enzyme": "3.10.9",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/history": "3.2.3",
    "@types/jest": "28.1.6",
    "@types/react-redux": "6.0.9",
    "@types/react-router": "3.0.24",
    "@types/react-router-redux": "4.0.49",
    "babel-jest": "28.0.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "jest": "28.1.3",
    "jest-cli": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-teamcity-reporter": "0.9.0",
    "react-test-renderer": "16.14.0",
    "ts-jest": "28.0.7",
    "xhr-mock": "2.5.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "node_modules/@labkey/build/webpack/tsconfig.json",
        "isolatedModules": true
      },
      "LABKEY": {
        "container": {
          "formats": {
            "dateFormat": "yyyy-MM-dd",
            "dateTimeFormat": "yyyy-MM-dd HH:mm"
          }
        },
        "moduleContext": {
          "samplemanagement": {
            "hasFreezerManagementEnabled": true
          },
          "inventory": {}
        },
        "project": {
          "rootId": "ROOTID"
        },
        "user": {
          "id": 1004
        }
      }
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/js/setup.ts"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test/globals.js",
      "/packages/workflow/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "testResultsProcessor": "jest-teamcity-reporter",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}