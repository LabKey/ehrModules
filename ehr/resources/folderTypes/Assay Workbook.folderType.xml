<folderType xmlns="http://labkey.org/data/xml/folderType">
    <name>Assay Workbook</name>
    <description>Workbook template for assay runs</description>
    <requiredWebParts>
        <webPart>
            <name>Workbook Description</name>
            <location>body</location>
        </webPart>
    </requiredWebParts>
    <preferredWebParts>
        <webPart>
            <name>Experiment Runs</name>
            <location>body</location>
        </webPart>
    </preferredWebParts>
    <modules>
        <moduleName>Pipeline</moduleName>
        <moduleName>Query</moduleName>
        <moduleName>Portal</moduleName>
    </modules>
    <defaultModule>Portal</defaultModule>
    <workbookType>true</workbookType>
</folderType>