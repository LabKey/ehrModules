<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="projectTotalAssigned" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="project">
                        <isKeyField>true</isKeyField>
                    </column>
                    <column columnName="ActiveAssignments">
                        <columnTitle>Animals Assigned</columnTitle>
                        <datatype>integer</datatype>
                        <url>/ehr/executeQuery.view?schemaName=study&amp;
                            query.queryName=Assignment&amp;
                            query.project~eq=${project}&amp;
                            query.sort=-Date&amp;
                            query.rdate~isblank
                        </url>
                    </column>
                </columns>
                <titleColumn>Animals Assigned</titleColumn>
            </table>
        </tables>
    </metadata>
</query>
