<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="protocol" tableDbType="TABLE">
                <tableUrl>/EHR/protocolDetails.view?key=${protocol}</tableUrl>
                <columns>
                    <column columnName="totalAnimals" wrappedColumnName="protocol">
                      <columnTitle>Total Animals</columnTitle>
                      <isUnselectable>true</isUnselectable>
                      <fk>
                        <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                        <fkDbSchema>lists</fkDbSchema>
                        <fkTable>protocolTotalAnimals</fkTable>
                        <fkColumnName>protocol</fkColumnName>
                      </fk>
                      <isHidden>false</isHidden>
                    </column>
                    <column columnName="totalProjects" wrappedColumnName="protocol">
                      <columnTitle>Total Projects</columnTitle>
                      <isUnselectable>true</isUnselectable>
                      <fk>
                        <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                        <fkDbSchema>lists</fkDbSchema>
                        <fkTable>protocolTotalProjects</fkTable>
                        <fkColumnName>protocol</fkColumnName>
                      </fk>
                      <isHidden>false</isHidden>
                    </column>
                    <column columnName="pdf" wrappedColumnName="protocol">
                        <columnTitle>PDF</columnTitle>
                        <url>http://www.primate.wisc.edu/colony-old/vetstaff/protocol.php?protocol=${protocol}</url>
                        <!--<url>/query/WNPRC/WNPRC_Units/Animal_Services/Compliance_Training/Private/Protocol_PDFs/executeQuery.view?schemaName=lists&query.queryName=ProtocolPDFs&protocol=${protocol}</url>-->
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
