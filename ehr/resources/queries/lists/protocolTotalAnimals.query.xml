<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="protocolTotalAnimals" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="protocol">
                        <isKeyField>true</isKeyField>
                        <fk>
                          <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                          <fkDbSchema>lists</fkDbSchema>
                          <fkTable>protocol</fkTable>
                          <fkColumnName>protocol</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="TotalAnimals">
                        <datatype>integer</datatype>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=assignment&amp;
                            query.project/protocol~eq=${protocol}&amp;   
                        </url>
                    </column>
                    <column columnName="ActiveAnimals">
                        <datatype>integer</datatype>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=assignment&amp;
                            query.project/protocol~eq=${protocol}&amp;
                            query.viewName=Active Assignments
                        </url>
                    </column>

                </columns>
                <titleColumn>TotalAnimals</titleColumn>
            </table>
        </tables>
    </metadata>
</query>
