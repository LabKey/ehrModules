<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="chemnorm" tableDbType="TABLE">
            <tableTitle>Chem Normals</tableTitle>
            <columns>
              <column columnName="testname">
                <datatype>varchar</datatype>
                <columnTitle>Test Name</columnTitle>
                <nullable>false</nullable>
                <isKeyField>true</isKeyField>
              </column>
              <column columnName="unit">
                <datatype>varchar</datatype>
                <columnTitle>Unit</columnTitle>
              </column>
              <column columnName="juv_m">
                <datatype>varchar</datatype>
                <columnTitle>Juvenile Male</columnTitle>
              </column>
              <column columnName="juv_f">
                <datatype>varchar</datatype>
                <columnTitle>Juvenile Female</columnTitle>
              </column>
              <column columnName="adult_m">
                <datatype>varchar</datatype>
                <columnTitle>Adult Male</columnTitle>
              </column>
              <column columnName="adult_f">
                <datatype>varchar</datatype>
                <columnTitle>Adult Female</columnTitle>
              </column>
              <column columnName="ger_m">
                <datatype>varchar</datatype>
                <columnTitle>Geriatric Male</columnTitle>
              </column>
              <column columnName="ger_f">
                <datatype>varchar</datatype>
                <columnTitle>Geriatric Female</columnTitle>
              </column>
              <column columnName="species">
                <datatype>varchar</datatype>
                <columnTitle>Species</columnTitle>
                <fk>
                  <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                  <fkDbSchema>ehr_lookups</fkDbSchema>
                  <fkTable>species</fkTable>
                  <fkColumnName>common</fkColumnName>
                </fk>
              </column>
              <column columnName="objectid">
                <datatype>varchar</datatype>
                <columnTitle>Key</columnTitle>
                <shownInInsertView>false</shownInInsertView>
                <shownInUpdateView>false</shownInUpdateView>
                <shownInDetailsView>false</shownInDetailsView>
                <!--<isKeyField>true</isKeyField>-->
                <isHidden>true</isHidden>
              </column>
              <column columnName="ts">
                <datatype>timestamp</datatype>
                <columnTitle>Last Changed</columnTitle>
                <!--<nullable>false</nullable>-->
                <shownInInsertView>false</shownInInsertView>
                <shownInUpdateView>false</shownInUpdateView>
                <shownInDetailsView>false</shownInDetailsView>
                <isHidden>true</isHidden>
              </column>
            </columns>
            <pkColumnName>testname</pkColumnName>
            </table>
        </tables>
    </metadata>
</query>
