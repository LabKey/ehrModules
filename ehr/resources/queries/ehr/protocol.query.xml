<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="protocol" tableDbType="TABLE">
                <tableUrl>/EHR/protocolDetails.view?key=${protocol}</tableUrl>
                <insertUrl />
                <importUrl />
                <updateUrl />
                <deleteUrl />
                <columns>
                    <column columnName="activeAnimals" wrappedColumnName="protocol">
                      <columnTitle>Active Animals</columnTitle>
                      <isUnselectable>true</isUnselectable>
                      <fk>
                        <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                        <fkDbSchema>ehr</fkDbSchema>
                        <fkTable>protocolActiveAnimals</fkTable>
                        <fkColumnName>protocol</fkColumnName>
                      </fk>
                      <isHidden>false</isHidden>
                    </column>
                    <column columnName="inves">
                        <columnTitle>Investigator</columnTitle>
                    </column>
                    <column columnName="totalProjects" wrappedColumnName="protocol">
                      <columnTitle>Total Projects</columnTitle>
                      <isUnselectable>true</isUnselectable>
                      <fk>
                        <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                        <fkDbSchema>ehr</fkDbSchema>
                        <fkTable>protocolTotalProjects</fkTable>
                        <fkColumnName>protocol</fkColumnName>
                      </fk>
                      <isHidden>false</isHidden>
                    </column>
                    <column columnName="pdf" wrappedColumnName="protocol">
                        <columnTitle>PDF</columnTitle>
                        <url>/query/WNPRC/WNPRC_Units/Animal_Services/Compliance_Training/Private/Protocol_PDFs/executeQuery.view?schemaName=lists&amp;query.queryName=ProtocolPDFs&amp;query.protocol~eq=${protocol}&amp;</url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
