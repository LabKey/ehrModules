 <query xmlns="http://labkey.org/data/xml/query">
    <metadata> 
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="problem" tableDbType="TABLE">
                <tableUrl>/query/detailsQueryRow.view?schemaName=study&amp;query.queryName=Problem List&amp;lsid=${lsid}</tableUrl>
                <!--<insertUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Problem List&amp;keyField=lsid&amp;</insertUrl>-->
                <!--<updateUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Problem List&amp;keyField=lsid&amp;key=${lsid}</updateUrl>-->
                <insertUrl />
                <importUrl />
                <updateUrl />
                <deleteUrl />
                <columns>
                    <column columnName="date">
                        <columnTitle>Date Observed</columnTitle>
                        <formatString>yyyy-MM-dd</formatString>
                    </column>
                    <column columnName="parentid">
                        <!--<fk>-->
                          <!--<fkDbSchema>study</fkDbSchema>-->
                          <!--<fkTable>studyDataParent</fkTable>-->
                          <!--<fkColumnName>objectid</fkColumnName>-->
                        <!--</fk>-->
                    </column>
                    <column columnName="problem_no">
                        <columnTitle>Problem Number</columnTitle>
                    </column>
                    <column columnName="code">
                        <columnTitle>Code</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>snomed</fkTable>
                            <fkColumnName>code</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="category">
                        <columnTitle>Category</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>problem_list_category</fkTable>
                            <fkColumnName>category</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="enddate">
                        <columnTitle>Date Resolved</columnTitle>
                        <formatString>yyyy-MM-dd</formatString>
                        <isHidden>false</isHidden>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>