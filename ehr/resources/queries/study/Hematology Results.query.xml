 <query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="hematologyResults" tableDbType="TABLE">
                <tableUrl>/query/detailsQueryRow.view?schemaName=study&amp;query.queryName=hematologyResults&amp;lsid=${lsid}</tableUrl>
                <!--<insertUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Hematology Results&amp;keyField=lsid&amp;</insertUrl>-->
                <!--<updateUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Hematology Results&amp;keyField=lsid&amp;key=${lsid}</updateUrl>-->
                <insertUrl />
                <importUrl />
                <updateUrl />
                <deleteUrl />
                <columns>
                    <column columnName="RefRange" wrappedColumnName="lsid">
                        <columnTitle>Ref Range</columnTitle>
                        <isHidden>false</isHidden>
                        <isUnselectable>true</isUnselectable>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>hematologyRefRange</fkTable>
                            <fkColumnName>lsid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="qualResult">
                        <displayWidth>120</displayWidth>
                    </column>
                    <column columnName="units">
                        <displayWidth>60</displayWidth>
                    </column>
                    <column columnName="parentid">
                        <!--<fk>-->
                          <!--<fkDbSchema>study</fkDbSchema>-->
                          <!--<fkTable>hematologyRuns</fkTable>-->
                          <!--<fkColumnName>objectid</fkColumnName>-->
                        <!--</fk>-->
                    </column>
                    <column columnName="runId" wrappedColumnName="parentid">
                        <isHidden>false</isHidden>
                        <fk>
                          <fkDbSchema>study</fkDbSchema>
                          <fkTable>clinpathRunsParent</fkTable>
                          <fkColumnName>objectid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="result">
                        <columnTitle>Result</columnTitle>
                    </column>
                    <column columnName="testid">
                      <!--<nullable>false</nullable>-->
                      <columnTitle>Test Id</columnTitle>
                        <displayWidth>120</displayWidth>
                      <fk>
                        <fkDbSchema>ehr_lookups</fkDbSchema>
                        <fkTable>hematology_tests</fkTable>
                        <fkColumnName>testid</fkColumnName>
                      </fk>
                    </column>
                    <column columnName="date">
                        <formatString>yyyy-MM-dd</formatString>
                    </column>
                    <column columnName="method">
                      <!--<nullable>false</nullable>-->
                      <columnTitle>Method</columnTitle>
                      <fk>
                        <fkDbSchema>ehr_lookups</fkDbSchema>
                        <fkTable>hematology_method</fkTable>
                        <fkColumnName>method</fkColumnName>
                      </fk>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>