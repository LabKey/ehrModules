<query xmlns="http://labkey.org/data/xml/query" name="Colony Population By Age">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="colonyPopulationByAge" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="species">
                        <datatype>varchar</datatype>
                        <columnTitle>Species</columnTitle>
                        <fk>
                          <fkDbSchema>lists</fkDbSchema>
                          <fkTable>species</fkTable>
                          <fkColumnName>common</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="animalCount">
                        <datatype>integer</datatype>
                        <columnTitle>Animal Count</columnTitle>
                         <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=demographics&amp;
                            query.Id/Species/Species~eq=${species}&amp;
                            query.Id/Age/AgeInYears~eq=${AgeInYearsRounded}&amp;
                            query.gender~eq=${gender}&amp;
                            query.viewName=Alive%2C at WNPRC
                        </url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
