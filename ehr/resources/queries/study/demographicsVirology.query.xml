<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsVirology" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="Id">
                        <isKeyField>true</isKeyField>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="numResults">
                        <columnTitle>Number of Results</columnTitle>
                        <url>/ehr/executeQuery.view?schemaName=study&amp;
                            query.queryName=Virology Results&amp;
                            query.Id~eq=${Id}
                        </url>
                    </column>
                </columns>
            <titleColumn>numResults</titleColumn>
            </table>
        </tables>
    </metadata>
</query>
