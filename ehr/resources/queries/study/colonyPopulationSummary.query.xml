<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="colonyPopulationSummary" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="species">
                        <datatype>varchar</datatype>
                        <columnTitle>Species</columnTitle>
                        <fk>
                          <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                          <fkDbSchema>ehr_lookups</fkDbSchema>
                          <fkTable>species</fkTable>
                          <fkColumnName>common</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="AnimalCount">
                        <datatype>varchar</datatype>
                        <columnTitle>Animal Count</columnTitle>
                         <url>/ehr/executeQuery.view?schemaName=study&amp;
                            query.queryName=demographics&amp;
                            query.Id/Species/Species~eq=${species}&amp;
                            query.viewName=Alive%2C at WNPRC
                        </url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
