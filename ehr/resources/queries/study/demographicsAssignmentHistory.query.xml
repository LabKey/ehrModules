<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsAssignmentHistory" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="Id">
                        <isKeyField>true</isKeyField>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="HistoricAssignments">
                        <datatype>integer</datatype>
                        <columnTitle>Historic Assignments (avail=all)</columnTitle>
                        <displayWidth>110</displayWidth>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=Assignment&amp;
                            query.Id~eq=${Id}&amp;
                            query.rdate~isblank&amp;
                        </url>
                    </column>
                    <column columnName="PendingAssignments">
                        <datatype>integer</datatype>
                        <columnTitle>Total Pending Assignments (avail=p)</columnTitle>
                        <displayWidth>110</displayWidth>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=Assignment&amp;
                            query.Id~eq=${Id}&amp;
                            query.rdate~isblank&amp;
                            query.project/avail~eq=p
                        </url>
                    </column>
                    <column columnName="HistoricResearchAssignments">
                        <datatype>integer</datatype>
                        <columnTitle>Total Historic Research Assignments (avail=r or n)</columnTitle>
                        <displayWidth>110</displayWidth>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=Assignment&amp;
                            query.Id~eq=${Id}&amp;
                            query.rdate~isblank&amp;
                            query.project/avail~in=r;e;n&amp;
                        </url>
                    </column>
                    <column columnName="HistoricBreedingAssignments">
                        <datatype>varchar</datatype>
                        <columnTitle>Total Historic Breeding Assignments (avail=b)</columnTitle>
                        <displayWidth>110</displayWidth>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=Assignment&amp;
                            query.Id~eq=${Id}&amp;
                            query.rdate~isblank&amp;
                            query.project/avail~eq=b
                        </url>
                    </column>
                    <column columnName="HistoricVetAssignments">
                        <datatype>varchar</datatype>
                        <columnTitle>Total Historic Vet Exemptions (avail=v)</columnTitle>
                        <displayWidth>110</displayWidth>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=Assignment&amp;
                            query.Id~eq=${Id}&amp;
                            query.rdate~isblank&amp;
                            query.project/avail~eq=v
                        </url>
                    </column>
                    <column columnName="HistoricTrainingAssignments">
                        <datatype>varchar</datatype>
                        <columnTitle>Total Historic Training Assignments (avail=t)</columnTitle>
                        <displayWidth>110</displayWidth>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=Assignment&amp;
                            query.Id~eq=${Id}&amp;
                            query.rdate~isblank&amp;
                            query.project/avail~eq=t
                        </url>
                    </column>
                    <column columnName="HistoricSPF_StockAssignment">
                        <datatype>varchar</datatype>
                        <columnTitle>Assigned to SPF Stock?</columnTitle>
                        <displayWidth>110</displayWidth>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=Assignment&amp;
                            query.Id~eq=${Id}&amp;
                            query.rdate~isblank&amp;
                            query.project~eq=20020201
                        </url>
                    </column>
                    <column columnName="HistoricConventionalStockAssignment">
                        <datatype>varchar</datatype>
                        <columnTitle>Assigned to Conventional Stock?</columnTitle>
                        <displayWidth>110</displayWidth>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=Assignment&amp;
                            query.Id~eq=${Id}&amp;
                            query.rdate~isblank&amp;
                            query.project~eq=20070202
                        </url>
                    </column>
                    <column columnName="HistoricMarmStockAssignment">
                        <datatype>varchar</datatype>
                        <columnTitle>Assigned to Marm Stock?</columnTitle>
                        <displayWidth>110</displayWidth>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=Assignment&amp;
                            query.Id~eq=${Id}&amp;
                            query.rdate~isblank&amp;
                            query.project~eq=20070801
                        </url>
                    </column>
                    <column columnName="HistoricStockAssignments">
                        <datatype>integer</datatype>
                        <columnTitle>Historic Stock Assignments (any)?</columnTitle>
                        <displayWidth>110</displayWidth>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=Assignment&amp;
                            query.Id~eq=${Id}&amp;
                            query.rdate~isblank&amp;
                            query.project~in=20020201;20070801;20070202
                        </url>
                    </column>
                    <column columnName="HistoricAssignments2">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="PendingAssignments2">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="HistoricResearchAssignments2">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="HistoricVetAssignments2">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="HistoricTrainingAssignments2">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="HistoricSPF_StockAssignments2">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="HistoricConventionalStockAssignments2">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="HistoricMarmStockAssignments2">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="HistoricStockAssignments2">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="HistoricBreedingAssignments2">
                        <isHidden>true</isHidden>
                    </column>
                </columns>
                <titleColumn>HistoricAssignments</titleColumn>
            </table>
        </tables>
    </metadata>
</query>
