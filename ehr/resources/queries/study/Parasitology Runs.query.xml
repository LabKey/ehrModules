<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="parasitologyRuns" tableDbType="TABLE">
                <tableUrl>/EHR/clinpathDetails.view?runId=${objectid}&amp;dataset=${dataset}</tableUrl>
                <!--<insertUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Parasitology Runs&amp;keyField=lsid&amp;</insertUrl>-->
                <!--<updateUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Parasitology Runs&amp;keyField=lsid&amp;key=${lsid}</updateUrl>-->
                <insertUrl />
                <updateUrl />
                <deleteUrl />
                <columns>
                    <column columnName="room">
                        <datatype>varchar</datatype>
                        <columnTitle>Room</columnTitle>
                        <url>/EHR/cageDetails.view?
                            room=${room}&amp;
                            cage=${cage}
                        </url>
                    </column>
                    <column columnName="parentId">
                        <fk>
                          <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                          <fkDbSchema>study</fkDbSchema>
                          <fkTable>studyDataParent</fkTable>
                          <fkColumnName>objectid</fkColumnName>
                        </fk>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>