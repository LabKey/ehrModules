<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="tissue_samples" tableDbType="TABLE">
                <tableUrl>/query/detailsQueryRow.view?schemaName=study&amp;query.queryName=tissue_samples&amp;lsid=${lsid}</tableUrl>
                <!--<insertUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Tissue Samples&amp;keyField=lsid&amp;</insertUrl>-->
                <!--<updateUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Tissue Samples&amp;keyField=lsid&amp;key=${lsid}</updateUrl>-->
                <insertUrl />
                <importUrl />
                <updateUrl />
                <deleteUrl />
                <columns>
                    <column columnName="qualifier">
                      <datatype>varchar</datatype>
                      <columnTitle>Qualifier</columnTitle>
                      <fk>
                        <fkDbSchema>ehr_lookups</fkDbSchema>
                        <fkTable>snomed_qualifiers</fkTable>
                        <fkColumnName>qualifier</fkColumnName>
                      </fk>
                    </column>
                    <column columnName="preservation">
                      <datatype>varchar</datatype>
                      <columnTitle>Preservation</columnTitle>
                      <fk>
                        <fkDbSchema>ehr_lookups</fkDbSchema>
                        <fkTable>preservation_solutions</fkTable>
                        <fkColumnName>solution</fkColumnName>
                      </fk>
                    </column>
                    <column columnName="tissue">
                      <datatype>varchar</datatype>
                      <columnTitle>Organ/Tissue</columnTitle>
                      <fk>
                        <fkDbSchema>ehr_lookups</fkDbSchema>
                        <fkTable>snomed</fkTable>
                        <fkColumnName>code</fkColumnName>
                      </fk>
                    </column>
                    <column columnName="container_type">
                        <columnTitle>Container Type</columnTitle>
                          <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>container_types</fkTable>
                            <fkColumnName>type</fkColumnName>
                          </fk>
                    </column>
                    <column columnName="ship_to">
                        <columnTitle>Ship To</columnTitle>
                    </column>
                    <column columnName="accountToCharge">
                        <columnTitle>Account To Charge</columnTitle>
                    </column>
                    <!--<column columnName="container_type">-->
                        <!--<columnTitle>Container Type</columnTitle>-->
                    <!--</column>                    -->
                </columns>
            </table>
        </tables>
    </metadata>
</query>