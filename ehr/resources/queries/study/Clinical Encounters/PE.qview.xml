<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="Date"/>
        <column name="Enddate"/>
        <!--<column name="caseno"/>-->
        <column name="project"/>
        <column name="account"/>
        <column name="type"/>
        <column name="title"/>
        <column name="major"/>
        <column name="performedby"/>

        <column name="Remark"/>
        <!--<column name="description"/>-->
        <column name="qcstate" />
    </columns>
    <sorts>
        <sort column="date" descending="true"/>
    </sorts>
    <filters>
        <filter column="type" operator="eq" value="Physical Exam"/>
    </filters>
</customView>