<customView xmlns="http://labkey.org/data/xml/queryCustomView">
   <columns>
       <column name="Id" />
       <column name="date" />
       <column name="project" />
       <column name="project/protocol">
            <properties>
                <property name="columnTitle" value="Protocol"/>
            </properties>
       </column>
       <column name="quantity" />
       <column name="performedby" />
       <column name="requestor" />
       <column name="p_s" />
       <column name="a_v" />
       <column name="sampleId" />
       <column name="caretaker" />
       <column name="tube_type" />
       <column name="Remark" />
       <column name="BloodRemaining/BloodLast30">
            <properties>
                <property name="columnTitle" value="Drawn in Previous 30 Days"/>
            </properties>
       </column>
       <column name="BloodRemaining/weight">
            <properties>
                <property name="columnTitle" value="Latest Weight (kg)"/>
            </properties>
       </column>
       <column name="BloodRemaining/MaxBlood">
            <properties>
                <property name="columnTitle" value="Max Allowable Blood"/>
            </properties>
       </column>
       <column name="BloodRemaining/AvailBlood">
            <properties>
                <property name="columnTitle" value="Volume Remaining (mL)"/>
            </properties>
       </column>
   </columns>
   <!--<filters>-->
       <!--<filter column="date" operator="isblank"/>-->
   <!--</filters>-->
   <sorts>
       <sort column="date" descending="true"/>
   </sorts>
</customView>