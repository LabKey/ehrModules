<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="curLocation/Room">
            <properties>
                <property name="columnTitle" value="Room"/>
            </properties>
        </column>
        <column name="curLocation/Cage">
            <properties>
                <property name="columnTitle" value="Cage"/>
            </properties>
        </column>
        <column name="curLocation/Cond">
            <properties>
                <property name="columnTitle" value="Condition"/>
            </properties>
        </column>
        <column name="Dataset/Demographics/Gender">
            <properties>
                <property name="columnTitle" value="Gender"/>
            </properties>
        </column>
        <!--<column name="Dataset/Demographics/Status">-->
            <!--<properties>-->
                <!--<property name="columnTitle" value="Status"/>-->
            <!--</properties>-->
        <!--</column>-->
        <column name="Dataset/Demographics/calculated_status">
            <properties>
                <property name="columnTitle" value="Status"/>
            </properties>
        </column>
        <column name="DaysAlone/DaysAlone">
            <properties>
                <property name="columnTitle" value="Days Alone"/>
            </properties>
        </column>
        <column name="DaysAlone/AloneSince">
            <properties>
                <property name="columnTitle" value="Alone Since"/>
            </properties>
        </column>
        <column name="DaysAlone/Exemptions">
            <properties>
                <property name="columnTitle" value="Exemptions"/>
            </properties>
        </column>
    </columns>
    <filters>
        <filter column="DaysAlone/DaysAlone" operator="neq" value="0"/>
    </filters>
    <sorts>
        <sort column="DaysAlone/DaysAlone" descending="true"/>
        <sort column="id" descending="false"/>
    </sorts>
</customView>