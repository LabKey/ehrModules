<query xmlns="http://labkey.org/data/xml/query">
  <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="StudyData" tableDbType="TABLE">
                <pkColumnName>lsid</pkColumnName>
                <columns>
                    <column columnName="lsid">
                        <isKeyField>true</isKeyField>
                        <shownInDetailsView>false</shownInDetailsView>
                    </column>
                    <column columnName="remark">
                        <isHidden>false</isHidden>
                        <displayWidth>350</displayWidth>
                    </column>
                    <column columnName="description">
                        <isHidden>false</isHidden>
                        <displayWidth>300</displayWidth>
                    </column>
                    <column columnName="enddate">
                        <columnTitle>End Date</columnTitle>
                        <formatString>yyyy-MM-dd HH:mm</formatString>
                    </column>
                    <column columnName="date">
                        <formatString>yyyy-MM-dd HH:mm</formatString>
                    </column>
                    <column columnName="project">
                        <fk>
                            <fkDbSchema>ehr</fkDbSchema>
                            <fkTable>project</fkTable>
                            <fkColumnName>project</fkColumnName>
                        </fk>
                        <formatString>00000000</formatString>
                    </column>
                    <column columnName="parentid">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="account">
                        <displayWidth>60</displayWidth>
                    </column>
                    <column columnName="userid">
                        <displayWidth>100</displayWidth>
                    </column>
                    <column columnName="AgeAtTime" wrappedColumnName="lsid">
                        <columnTitle>Age At The Time</columnTitle>
                        <isHidden>false</isHidden>
                        <isUnselectable>true</isUnselectable>
                        <shownInDetailsView>false</shownInDetailsView>
                        <shownInInsertView>false</shownInInsertView>
                        <shownInUpdateView>false</shownInUpdateView>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>AgeAtTime</fkTable>
                            <fkColumnName>lsid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="DateOnly" wrappedColumnName="lsid">
                        <columnTitle>Date Only</columnTitle>
                        <shownInDetailsView>false</shownInDetailsView>
                        <shownInInsertView>false</shownInInsertView>
                        <shownInUpdateView>false</shownInUpdateView>
                        <isHidden>false</isHidden>
                        <isUnselectable>true</isUnselectable>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>studyDataDateOnly</fkTable>
                            <fkColumnName>lsid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="Notes" wrappedColumnName="date">
                        <columnTitle>Notes</columnTitle>
                        <nullable>true</nullable>
                        <formatString>'&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;'</formatString>
                        <shownInDetailsView>false</shownInDetailsView>
                        <shownInInsertView>false</shownInInsertView>
                        <shownInUpdateView>false</shownInUpdateView>
                        <isHidden>false</isHidden>
                        <displayWidth>200</displayWidth>
                    </column>
                    <column columnName="Survivorship" wrappedColumnName="lsid">
                        <columnTitle>Survivorship</columnTitle>
                        <isHidden>false</isHidden>
                        <isUnselectable>true</isUnselectable>
                        <shownInDetailsView>false</shownInDetailsView>
                        <shownInInsertView>false</shownInInsertView>
                        <shownInUpdateView>false</shownInUpdateView>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>studyDataSurvivorship</fkTable>
                            <fkColumnName>lsid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="HousingAtTime" wrappedColumnName="lsid">
                        <columnTitle>Housing At Time</columnTitle>
                        <isHidden>false</isHidden>
                        <isUnselectable>true</isUnselectable>
                        <shownInDetailsView>false</shownInDetailsView>
                        <shownInInsertView>false</shownInInsertView>
                        <shownInUpdateView>false</shownInUpdateView>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>studyDataRoomAtTime</fkTable>
                            <fkColumnName>lsid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="AssignmentAtTime" wrappedColumnName="lsid">
                        <columnTitle>Assignment At Time</columnTitle>
                        <isHidden>false</isHidden>
                        <isUnselectable>true</isUnselectable>
                        <shownInDetailsView>false</shownInDetailsView>
                        <shownInInsertView>false</shownInInsertView>
                        <shownInUpdateView>false</shownInUpdateView>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>studyDataAssignmentAtTime</fkTable>
                            <fkColumnName>lsid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="taskid">
                        <isHidden>false</isHidden>
                        <columnTitle>Task Id</columnTitle>
                        <fk>
                            <fkDbSchema>ehr</fkDbSchema>
                            <fkTable>tasks</fkTable>
                            <fkColumnName>taskid</fkColumnName>
                        </fk>
                        <url>/ehr/taskDetails.view?formtype=${taskid/formtype}&amp;taskid=${taskid}</url>
                    </column>
                    <column columnName="requestid">
                        <isHidden>false</isHidden>
                        <columnTitle>Request Id</columnTitle>
                        <fk>
                            <fkDbSchema>ehr</fkDbSchema>
                            <fkTable>requests</fkTable>
                            <fkColumnName>requestid</fkColumnName>
                        </fk>
                        <url>/ehr/requestDetails.view?formtype=${requestid/formtype}&amp;requestid=${requestid}</url>
                    </column>
                    <column columnName="QCState">
                        <isHidden>false</isHidden>
                        <shownInDetailsView>true</shownInDetailsView>
                        <columnTitle>Status</columnTitle>
                        <displayWidth>50</displayWidth>
                        <!--<url>/ehr/taskDetails.view?formtype=${taskid/formtype}&amp;taskid=${taskid}</url>-->
                        <conditionalFormats>
                            <conditionalFormat>
                                <filters>
                                    <filter operator="eq" value="In Progress"/>
                                </filters>
                                <backgroundColor>FBEC5D</backgroundColor>
                            </conditionalFormat>
                            <conditionalFormat>
                                <filters>
                                    <filter operator="eq" value="Review Required"/>
                                </filters>
                                <backgroundColor>FBEC5D</backgroundColor>
                            </conditionalFormat>
                            <conditionalFormat>
                                <filters>
                                    <filter operator="eq" value="Scheduled"/>
                                </filters>
                                <backgroundColor>FBEC5D</backgroundColor>
                            </conditionalFormat>
                            <conditionalFormat>
                                <filters>
                                    <filter operator="eq" value="Delete Requested"/>
                                </filters>
                                <backgroundColor>FF0000</backgroundColor>
                            </conditionalFormat>
                            <conditionalFormat>
                                <filters>
                                    <filter operator="eq" value="Request: Approved"/>
                                </filters>
                                <backgroundColor>FBEC5D</backgroundColor>
                            </conditionalFormat>
                            <conditionalFormat>
                                <filters>
                                    <filter operator="eq" value="Request: Denied"/>
                                </filters>
                                <backgroundColor>FF0000</backgroundColor>
                            </conditionalFormat>
                            <conditionalFormat>
                                <filters>
                                    <filter operator="eq" value="Request: Pending"/>
                                </filters>
                                <backgroundColor>FBEC5D</backgroundColor>
                            </conditionalFormat>
                        </conditionalFormats>
                    </column>
                    <column columnName="animalvisit">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="sequencenum">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="created">
                        <isHidden>true</isHidden>
                        <formatString>yyyy-MM-dd HH:mm</formatString>
                    </column>
                    <column columnName="createdby">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="modified">
                        <isHidden>true</isHidden>
                        <formatString>yyyy-MM-dd HH:mm</formatString>
                    </column>
                    <column columnName="modifiedby">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="ts">
                        <formatString>yyyy-MM-dd HH:mm</formatString>
                    </column>

                    <!--<column columnName="ParentRecord" wrappedColumnName="objectid">-->
                        <!--<columnTitle>Parent Record</columnTitle>-->
                        <!--<isHidden>false</isHidden>-->
                        <!--<isUnselectable>true</isUnselectable>-->
                        <!--<shownInDetailsView>false</shownInDetailsView>-->
                        <!--<shownInInsertView>false</shownInInsertView>-->
                        <!--<shownInUpdateView>false</shownInUpdateView>-->
                        <!--<fk>-->
                            <!--<fkDbSchema>study</fkDbSchema>-->
                            <!--<fkTable>studyDataParent</fkTable>-->
                            <!--<fkColumnName>objectid</fkColumnName>-->
                        <!--</fk>-->
                    <!--</column>-->
                </columns>
                <buttonBarOptions position="both" includeStandardButtons="false">
                    <includeScript>/ehr/studyButtons.js</includeScript>
                    <onRender>EHR.DatasetButtons.moreActionsHandler</onRender>
                    <item text="Views">
                        <originalText>Views</originalText>
                    </item>
                    <item text="Insert New">
                        <originalText>Insert New</originalText>
                    </item>
                    <!--<item text="Cohorts">-->
                        <!--<originalText>Cohorts</originalText>-->
                    <!--</item>-->
                    <!--<item text="QCState">-->
                        <!--<originalText>QC State</originalText>-->
                    <!--</item>-->
                    <item text="Export">
                        <originalText>Export</originalText>
                    </item>
                    <item text="Print">
                        <originalText>Print</originalText>
                    </item>
                    <item text="Import Data">
                        <originalText>Import Data</originalText>
                    </item>
                    <item text="Page Size">
                        <originalText>Page Size</originalText>
                    </item>
                    <item text="Delete">
                        <originalText>Delete</originalText>
                    </item>
                    <item text="More Actions" insertPosition="end" >
                        <item text="Jump To History">
                            <onClick>
                                EHR.DatasetButtons.historyHandler(dataRegion, dataRegionName);
                            </onClick>
                            <!--<target>javascript:void(0)</target>-->
                        </item>
                        <item text="Jump To Other Dataset">
                            <onClick>
                                EHR.DatasetButtons.datasetHandler(dataRegion, dataRegionName);
                            </onClick>
                        </item>
                        <item text="Return Distinct Values">
                            <onClick>
                                EHR.DatasetButtons.getDistinctHandler(dataRegion, dataRegionName);
                            </onClick>
                        </item>
                        <item text="Show Record History">
                            <onClick>
                                EHR.DatasetButtons.showAuditHistoryHandler(dataRegion, dataRegionName);
                            </onClick>
                        </item>
                    </item>
                    <item text=" " requiresSelection="true" insertAfter="More Actions">
                        <onClick>function(){return false}</onClick>
                    </item>
                </buttonBarOptions>
            </table>
        </tables>
    </metadata>
</query>