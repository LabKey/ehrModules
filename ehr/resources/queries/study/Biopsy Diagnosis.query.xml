 <query xmlns="http://labkey.org/data/xml/query">
    <metadata> 
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="Biopsydiag" tableDbType="TABLE">
                <tableUrl>/query/detailsQueryRow.view?schemaName=study&amp;query.queryName=Biopsydiag&amp;lsid=${lsid}</tableUrl>
                <!--<insertUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Biopsy Diagnosis&amp;keyField=lsid&amp;</insertUrl>-->
                <!--<updateUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Biopsy Diagnosis&amp;keyField=lsid&amp;key=${lsid}</updateUrl>-->
                <insertUrl />
                <updateUrl />
                <deleteUrl />
                <columns>
                    <column columnName="parentId">
                        <fk>
                          <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                          <fkDbSchema>study</fkDbSchema>
                          <fkTable>biopsy</fkTable>
                          <fkColumnName>objectid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="code">
                        <datatype>varchar</datatype>
                        <columnTitle>SNOMED</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>snomed</fkTable>
                            <fkColumnName>code</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="tissue">
                      <datatype>varchar</datatype>
                      <columnTitle>Organ/Tissue</columnTitle>
                      <fk>
                        <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                        <fkDbSchema>ehr_lookups</fkDbSchema>
                        <fkTable>snomed</fkTable>
                        <fkColumnName>code</fkColumnName>
                      </fk>
                    </column>
                    <column columnName="severity">
                      <datatype>varchar</datatype>
                      <columnTitle>Severity</columnTitle>
                      <fk>
                        <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                        <fkDbSchema>ehr_lookups</fkDbSchema>
                        <fkTable>snomed</fkTable>
                        <fkColumnName>code</fkColumnName>
                      </fk>
                    </column>
                    <column columnName="diagnosis">
                      <datatype>varchar</datatype>
                      <columnTitle>Diagnosis</columnTitle>
                      <fk>
                        <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                        <fkDbSchema>ehr_lookups</fkDbSchema>
                        <fkTable>snomed</fkTable>
                        <fkColumnName>code</fkColumnName>
                      </fk>
                    </column>
                    <column columnName="seq1">
                      <datatype>integer</datatype>
                      <columnTitle>Diagnosis Number</columnTitle>
                      <url>/query/executeQuery.view?schemaName=study&amp;
                          query.queryName=Necropsy%20Diagnosis&amp;
                          query.id~eq=${id}&amp;
                          query.date~eq=${date}&amp;
                          query.seq1~eq=${seq1}&amp;
                      </url>
                    </column>
                    <column columnName="seq2">
                      <datatype>integer</datatype>
                      <columnTitle>Diagnosis Code Number</columnTitle>
                    </column>
                    <column columnName="duration">
                      <datatype>varchar</datatype>
                      <!--<nullable>false</nullable>-->
                      <columnTitle>Duration</columnTitle>
                      <fk>
                        <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                        <fkDbSchema>ehr_lookups</fkDbSchema>
                        <fkTable>snomed</fkTable>
                        <fkColumnName>code</fkColumnName>
                      </fk>
                    </column>
                    <column columnName="severity">
                      <datatype>varchar</datatype>
                      <!--<nullable>false</nullable>-->
                      <columnTitle>Severity</columnTitle>
                      <fk>
                        <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                        <fkDbSchema>ehr_lookups</fkDbSchema>
                        <fkTable>snomed</fkTable>
                        <fkColumnName>code</fkColumnName>
                      </fk>
                    </column>
                    <column columnName="process">
                      <datatype>varchar</datatype>
                      <!--<nullable>false</nullable>-->
                      <columnTitle>Process</columnTitle>
                      <fk>
                        <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                        <fkDbSchema>ehr_lookups</fkDbSchema>
                        <fkTable>snomed</fkTable>
                        <fkColumnName>code</fkColumnName>
                      </fk>
                    </column>
                    <column columnName="distribution">
                      <datatype>varchar</datatype>
                      <!--<nullable>false</nullable>-->
                      <columnTitle>Distribution</columnTitle>
                      <fk>
                        <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                        <fkDbSchema>ehr_lookups</fkDbSchema>
                        <fkTable>snomed</fkTable>
                        <fkColumnName>code</fkColumnName>
                      </fk>
                    </column>
                    <column columnName="biopsy" wrappedColumnName="parentId">
                      <datatype>varchar</datatype>
                      <isUnselectable>true</isUnselectable>
                      <columnTitle>Biopsy</columnTitle>
                      <fk>
                        <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                        <fkDbSchema>study</fkDbSchema>
                        <fkTable>biopsyParent</fkTable>
                        <fkColumnName>objectid</fkColumnName>
                      </fk>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>