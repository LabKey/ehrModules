 <query xmlns="http://labkey.org/data/xml/query">
    <metadata> 
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="behavetrem" tableDbType="TABLE">
                <tableUrl>/query/detailsQueryRow.view?schemaName=study&amp;query.queryName=behavetrem&amp;lsid=${lsid}</tableUrl>
                <columns>
                    <column columnName="date">
                        <formatString>yyyy-MM-dd HH:mm</formatString>
                    </column>
                    <column columnName="project">
                        <fk>
                          <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                          <fkDbSchema>lists</fkDbSchema>
                          <fkTable>project</fkTable>
                          <fkColumnName>project</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="category">
                        <fk>
                          <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                          <fkDbSchema>ehr_lookups</fkDbSchema>
                          <fkTable>behavior_category</fkTable>
                          <fkColumnName>category</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="parentId">
                        <fk>
                          <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                          <fkDbSchema>study</fkDbSchema>
                          <fkTable>encounters</fkTable>
                          <fkColumnName>objectid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="remark">
                        <displayWidth>350</displayWidth>
                    </column>
                    <!--<column columnName="code">-->
                        <!--<datatype>varchar</datatype>-->
                        <!--<columnTitle>SNOMED</columnTitle>-->
                        <!--<fk>-->
                            <!--<fkDbSchema>lists</fkDbSchema>-->
                            <!--<fkTable>snomed</fkTable>-->
                            <!--<fkColumnName>code</fkColumnName>-->
                        <!--</fk>-->
                    <!--</column>                    -->
                    <column columnName="so">
                        <columnTitle>Subjective/Objective</columnTitle>
                    </column>
                    <column columnName="a">
                        <columnTitle>Assessment</columnTitle>
                    </column>
                    <column columnName="p">
                        <columnTitle>Plan</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>