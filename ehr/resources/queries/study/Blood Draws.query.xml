        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="Blood Draws" tableDbType="TABLE">
                <tableUrl>/query/detailsQueryRow.view?schemaName=study&amp;query.queryName=blood&amp;lsid=${lsid}</tableUrl>
                <columns>
                    <column columnName="BloodRemaining" wrappedColumnName="lsid">
                        <columnTitle>Blood Remaining</columnTitle>
                        <isHidden>false</isHidden>
                        <isUnselectable>true</isUnselectable>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>bloodSummary</fkTable>
                            <fkColumnName>lsid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="project">
                        <fk>
                          <fkDbSchema>lists</fkDbSchema>
                          <fkTable>project</fkTable>
                          <fkColumnName>project</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="date">
                        <formatString>yyyy-MM-dd HH:mm</formatString>
                    </column>
