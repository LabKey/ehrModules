<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id" />
        <column name="date" />
        <column name="enddate" />
        <column name="category" />
        <column name="value" />
        <column name="remark" />
        <column name="qcstate" />
    </columns>
    <sorts>
        <sort column="date" descending="true"/>
    </sorts>
    <filters>
        <filter column="enddateCoalesced" operator="dategte" value="+0d"/>
    </filters>
</customView>
