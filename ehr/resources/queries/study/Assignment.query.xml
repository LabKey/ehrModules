 <query xmlns="http://labkey.org/data/xml/query">
    <metadata> 
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="Assignment" tableDbType="TABLE">
                <tableUrl>/query/detailsQueryRow.view?schemaName=study&amp;query.queryName=Assignment&amp;lsid=${lsid}</tableUrl>
                <!--<insertUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Assignment&amp;keyField=lsid&amp;</insertUrl>-->
                <!--<updateUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Assignment&amp;keyField=lsid&amp;key=${lsid}</updateUrl>-->
                <insertUrl />
                <updateUrl />
                <deleteUrl />
                <columns>
                    <column columnName="project">
                        <fk>
                          <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                          <fkDbSchema>lists</fkDbSchema>
                          <fkTable>project</fkTable>
                          <fkColumnName>project</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="date">
                        <formatString>yyyy-MM-dd</formatString>
                        <columnTitle>Assign Date</columnTitle>
                    </column>
                    <column columnName="rdate">
                        <formatString>yyyy-MM-dd</formatString>
                        <columnTitle>Release Date</columnTitle>
                    </column>
                    <column columnName="parentId">
                        <fk>
                          <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                          <fkDbSchema>study</fkDbSchema>
                          <fkTable>studyDataParent</fkTable>
                          <fkColumnName>objectid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="CoAssignments" wrappedColumnName="lsid">
                        <columnTitle>CoAssignments</columnTitle>
                        <isHidden>false</isHidden>
                        <isUnselectable>true</isUnselectable>
                        <fk>
                            <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>assignmentTotalCoAssigned</fkTable>
                            <fkColumnName>lsid</fkColumnName>
                        </fk>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>