<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="clintremDiarrheaSummary" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="Id">
                        <isKeyField>true</isKeyField>
                    </column>
                    <column columnName="Year">
                        <datatype>integer</datatype>
                        <columnTitle>Year</columnTitle>
                    </column>
                    <column columnName="Week">
                        <datatype>integer</datatype>
                        <columnTitle>Week</columnTitle>
                    </column>
                    <column columnName="MonthName">
                        <datatype>integer</datatype>
                        <columnTitle>Month Name</columnTitle>
                    </column>
                    <column columnName="Day">
                        <datatype>varchar</datatype>
                        <displayWidth>10</displayWidth>
                        <url>/ehr/executeQuery.view?schemaName=study&amp;
                            query.queryName=clintremDiarrhea&amp;
                            query.Id~eq=${id}&amp;
                            query.Year~eq=${year}&amp;
                            query.week~eq=${week}&amp;
                            query.monthName~eq=${monthName}&amp;
                        </url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
