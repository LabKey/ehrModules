<customView xmlns="http://labkey.org/data/xml/queryCustomView">
   <columns>
       <column name="id"/>
       <column name="Date"/>
       <column name="project"/>
       <column name="dataset/label">
            <properties>
                <property name="columnTitle" value="Dataset"/>
            </properties>
       </column>
       <column name="remark"/>
       <column name="description"/>
   </columns>
   <sorts>
       <sort column="date" descending="true"/>
   </sorts>
   <filters>
       <filter column="dataset/name" operator="in" value="assignments;behavetrem;biopsy;blood;clinremarks;drug;housing;necropsy;problem;weight;surgeries;tb;bacteriologyRuns;parasitologyRuns;virologyRuns;chemistryRuns;hematologyRuns;urinalysisRuns;immunologyRuns;obs"/>
       <filter column="description"  operator="neq" value="No Irregular Observations"/>
   </filters>
</customView>