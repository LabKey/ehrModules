 <query xmlns="http://labkey.org/data/xml/query">
    <metadata> 
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="Deaths" tableDbType="TABLE">
                <tableUrl>/query/detailsQueryRow.view?schemaName=study&amp;query.queryName=Deaths&amp;lsid=${lsid}</tableUrl>
                <columns>
                    <column columnName="parentId">
                        <fk>
                          <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                          <fkDbSchema>study</fkDbSchema>
                          <fkTable>encounters</fkTable>
                          <fkColumnName>objectid</fkColumnName>
                        </fk>
                    </column>
                  <column columnName="cause">
                    <datatype>varchar</datatype>
                    <columnTitle>Cause</columnTitle>
                    <fk>
                      <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                      <fkDbSchema>lists</fkDbSchema>
                      <fkTable>death_codes</fkTable>
                      <fkColumnName>Code</fkColumnName>
                    </fk>
                  </column>
                  <column columnName="necropsy">
                    <datatype>varchar</datatype>
                    <columnTitle>Necropsy</columnTitle>
                    <fk>
                      <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                      <fkDbSchema>study</fkDbSchema>
                      <fkTable>necropsy</fkTable>
                      <fkColumnName>caseno</fkColumnName>
                    </fk>
                  </column>
                  <column columnName="remark">
                    <datatype>varchar</datatype>
                    <!--<columnTitle>Remark</columnTitle>-->
                    <displayWidth>110</displayWidth>
                    <inputType>textarea</inputType>
                  </column>
                  <column columnName="objectid">
                    <datatype>entityid</datatype>
                    <columnTitle>Key</columnTitle>
                    <shownInInsertView>false</shownInInsertView>
                    <shownInUpdateView>false</shownInUpdateView>
                    <shownInDetailsView>false</shownInDetailsView>
                    <isHidden>true</isHidden>
                    <!--<isKeyField>true</isKeyField>-->
                  </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>