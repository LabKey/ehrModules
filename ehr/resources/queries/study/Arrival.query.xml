 <query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="Arrival" tableDbType="TABLE">
                <tableUrl>/query/detailsQueryRow.view?schemaName=study&amp;query.queryName=Arrival&amp;lsid=${lsid}</tableUrl>
                <!--<insertUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Arrival&amp;keyField=lsid&amp;</insertUrl>-->
                <!--<updateUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Arrival&amp;keyField=lsid&amp;key=${lsid}</updateUrl>-->
                <insertUrl />
                <importUrl />
                <updateUrl />
                <deleteUrl />
                <columns>
                    <column columnName="date">
                        <columnTitle>Arrival Date</columnTitle>
                        <formatString>yyyy-MM-dd HH:mm</formatString>
                    </column>
                    <column columnName="parentId">
                        <!--<fk>-->
                          <!--<fkDbSchema>study</fkDbSchema>-->
                          <!--<fkTable>studyDataParent</fkTable>-->
                          <!--<fkColumnName>objectid</fkColumnName>-->
                        <!--</fk>-->
                    </column>
                    <column columnName="initialRoom">
                        <nullable>true</nullable>
                        <isHidden>true</isHidden>
                        <columnTitle>Room</columnTitle>
                        <!--<url>/EHR/cageDetails.view?-->
                            <!--room=${room}&amp;-->
                            <!--cage=${cage}&amp;-->
                        <!--</url>-->
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>rooms</fkTable>
                            <fkColumnName>room</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="initialCage">
                        <nullable>true</nullable>
                        <isHidden>true</isHidden>
                        <columnTitle>Cage</columnTitle>
                        <url>/EHR/cageDetails.view?
                            room=${room}&amp;
                            cage=${cage}&amp;
                        </url>
                    </column>
                    <column columnName="initialCond">
                        <nullable>true</nullable>
                        <isHidden>true</isHidden>
                        <columnTitle>Condition</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>condition_codes</fkTable>
                            <fkColumnName>Code</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="species">
                        <columnTitle>Species</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>species</fkTable>
                            <fkColumnName>common</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="dam">
                        <columnTitle>Dam</columnTitle>
                        <!--<fk>-->
                            <!--<fkDbSchema>study</fkDbSchema>-->
                            <!--<fkTable>animal</fkTable>-->
                            <!--<fkColumnName>id</fkColumnName>-->
                        <!--</fk>-->
                        <url>/study/participant.view?
                            participantId=${dam}
                        </url>
                    </column>
                    <column columnName="sire">
                        <columnTitle>Sire</columnTitle>
                        <!--<fk>-->
                            <!--<fkDbSchema>study</fkDbSchema>-->
                            <!--<fkTable>animal</fkTable>-->
                            <!--<fkColumnName>id</fkColumnName>-->
                        <!--</fk>-->
                        <url>/study/participant.view?
                            participantId=${sire}
                        </url>
                    </column>
                    <column columnName="geoOrigin">
                        <columnTitle>Geographic Origin</columnTitle>
                    </column>
                    <column columnName="gender">
                        <columnTitle>Gender</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>gender_codes</fkTable>
                            <fkColumnName>Code</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="source">
                        <columnTitle>Source</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>origin_codes</fkTable>
                            <fkColumnName>Code</fkColumnName>
                        </fk>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>