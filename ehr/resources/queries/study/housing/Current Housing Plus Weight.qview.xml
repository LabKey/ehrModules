<customView xmlns="http://labkey.org/data/xml/queryCustomView">
   <columns>
        <column name="Id"/>
        <column name="Date"/>
        <column name="Odate"/>
        <column name="Room"/>
        <column name="Cage"/>
        <column name="Cond"/>
        <column name="Id/Dataset/Demographics/Status">
            <properties>
                <property name="columnTitle" value="Status"/>
            </properties>
        </column>
        <column name="Id/Dataset/Demographics/Avail">
            <properties>
                <property name="columnTitle" value="Avail"/>
            </properties>
        </column>
        <column name="Id/MostRecentWeight/MostRecentWeight">
            <properties>
                <property name="columnTitle" value="Most Recent Weight"/>
            </properties>
        </column>
        <column name="Id/MostRecentWeight/MostRecentWeightDate">
            <properties>
                <property name="columnTitle" value="Weight Date"/>
            </properties>
        </column>
        <column name="Id/MostRecentWeight/DaysSinceWeight">
            <properties>
                <property name="columnTitle" value="Days Since Weight"/>
            </properties>
        </column>
   </columns>
   <filters>
       <filter column="odate" operator="isblank"/>
   </filters>
   <sorts>
       <sort column="date" descending="true"/>
   </sorts>
</customView>