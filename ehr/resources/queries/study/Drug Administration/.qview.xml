<customView xmlns="http://labkey.org/data/xml/queryCustomView">
   <columns>
       <column name="Id"/>
       <column name="date"/>
       <column name="EndTime"/>
       <column name="project"/>
       <column name="code"/>
       <column name="code/code">
           <properties>
               <property name="columnTitle" value="SNOMED Code"/>
           </properties>
       </column>
       <column name="amount"/>
       <column name="units"/>
       <column name="route"/>
       <column name="Remark"/>
       <column name="category"/>
       <column name="HeaderDate"/>
   </columns>
   <sorts>
       <sort column="date" descending="true"/>
   </sorts>
    <filters>
        <filter column="qcstate/publicdata" operator="eq" value="true"/>
    </filters>
</customView>