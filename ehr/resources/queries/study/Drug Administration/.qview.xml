<customView xmlns="http://labkey.org/data/xml/queryCustomView">
   <columns>
       <column name="id"/>
       <column name="Date"/>
       <column name="BeginTime"/>
       <column name="EndTime"/>
       <column name="project"/>
       <column name="code"/>
       <column name="code/code">
           <properties>
               <property name="columnTitle" value="SNOMED Code"/>
           </properties>
       </column>
       <column name="amount"/>
       <column name="units"/>
       <column name="route"/>
       <column name="remark"/>
   </columns>
   <sorts>
       <sort column="date" descending="true"/>
   </sorts>
</customView>