<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="Birth" tableDbType="TABLE">
                <tableUrl>/query/detailsQueryRow.view?schemaName=study&amp;query.queryName=Birth&amp;lsid=${lsid}</tableUrl>
                <!--<insertUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Birth&amp;keyField=lsid&amp;</insertUrl>-->
                <!--<updateUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Birth&amp;keyField=lsid&amp;key=${lsid}</updateUrl>-->
                <insertUrl />
                <updateUrl />
                <deleteUrl />
                <columns>
                    <column columnName="gender">
                        <datatype>varchar</datatype>
                        <columnTitle>Gender</columnTitle>
                        <fk>
                            <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>gender_codes</fkTable>
                            <fkColumnName>Code</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="dam">
                        <datatype>varchar</datatype>
                        <!--<nullable>false</nullable>-->
                        <columnTitle>Dam</columnTitle>
                        <!--<fk>-->
                            <!--<fkFolderPath>/wnprc/ehr/</fkFolderPath>-->
                            <!--<fkDbSchema>study</fkDbSchema>-->
                            <!--<fkTable>animal</fkTable>-->
                            <!--<fkColumnName>id</fkColumnName>-->
                        <!--</fk>-->
                        <url>/study/participant.view?
                            participantId=${dam}
                        </url>
                    </column>
                    <column columnName="sire">
                        <datatype>varchar</datatype>
                        <columnTitle>Sire</columnTitle>
                        <!--<fk>-->
                            <!--<fkFolderPath>/wnprc/ehr/</fkFolderPath>-->
                            <!--<fkDbSchema>study</fkDbSchema>-->
                            <!--<fkTable>animal</fkTable>-->
                            <!--<fkColumnName>id</fkColumnName>-->
                        <!--</fk>-->
                        <url>/study/participant.view?
                            participantId=${sire}
                        </url>
                    </column>
                    <column columnName="room">
                        <datatype>varchar</datatype>
                        <columnTitle>Room</columnTitle>
                        <url>/EHR/cageDetails.view?
                            room=${room}&amp;
                            cage=${cage}
                        </url>
                    </column>
                    <column columnName="cage">
                        <datatype>varchar</datatype>
                        <columnTitle>Cage</columnTitle>
                        <url>/EHR/cageDetails.view?
                            room=${room}&amp;
                            cage=${cage}
                        </url>
                    </column>
                    <column columnName="cond">
                        <datatype>varchar</datatype>
                        <columnTitle>Condition</columnTitle>
                        <fk>
                            <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>condition_codes</fkTable>
                            <fkColumnName>Code</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="origin">
                        <datatype>varchar</datatype>
                        <columnTitle>Origin</columnTitle>
                        <fk>
                            <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>origin_codes</fkTable>
                            <fkColumnName>Code</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="parentId">
                        <fk>
                          <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                          <fkDbSchema>study</fkDbSchema>
                          <fkTable>studyDataParent</fkTable>
                          <fkColumnName>objectid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="estimated">
                        <columnTitle>Estimated Birthdate</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>