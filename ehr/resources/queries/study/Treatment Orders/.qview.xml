           <properties>
               <property name="columnTitle" value="Current Room"/>
           </properties>
       </column>
       <column name="Id/curLocation/cage">
           <properties>
               <property name="columnTitle" value="Current Cage"/>
           </properties>
       </column>
       <column name="date"/>
       <column name="enddate"/>
       <column name="frequency"/>
       <column name="project"/>
       <column name="code"/>
       <column name="volume"/>
       <column name="vol_units"/>
       <column name="concentration"/>
       <column name="conc_units"/>
