<customView xmlns="http://labkey.org/data/xml/queryCustomView">
   <columns>
       <column name="Id/curLocation/room">
           <properties>
               <property name="columnTitle" value="Current Room"/>
           </properties>
       </column>
       <column name="Id/curLocation/cage">
           <properties>
               <property name="columnTitle" value="Current Cage"/>
           </properties>
       </column>
       <column name="id"/>       
       <column name="date"/>
       <!--<column name="enddate"/>-->
       <column name="TimeOfDay"/>
       <column name="frequency"/>
       <column name="project"/>
       <column name="code"/>
       <column name="volume"/>
       <column name="vunits"/>
       <column name="conc"/>
       <column name="cunits"/>
       <column name="amount"/>
       <column name="units"/>
       <column name="route"/>
       <column name="remark"/>
       <column name="userid"/>
   </columns>
   <sorts>
       <sort column="date" descending="true"/>
       <sort column="TimeOfDay" descending="false"/>
       <sort column="Id/curLocation/cage" descending="false"/>
       <sort column="Id/curLocation/room" descending="false"/>
   </sorts>
   <filters>
       <filter column="enddate" operator="isblank"/>
   </filters>
</customView>