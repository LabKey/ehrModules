 <query xmlns="http://labkey.org/data/xml/query">
    <metadata> 
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="necropsydiag" tableDbType="TABLE">
                <tableUrl>/query/detailsQueryRow.view?schemaName=study&amp;query.queryName=necropsydiag&amp;lsid=${lsid}</tableUrl>
                <columns>
                  <column columnName="participantid">
                    <datatype>varchar</datatype>
                    <columnTitle>Participant ID</columnTitle>
                    <description>Participant identifier</description>
                    <nullable>false</nullable>
                  </column>
                  <column columnName="date">
                    <datatype>timestamp</datatype>
                    <formatString>yyyy-MM-dd</formatString>
                    <columnTitle>Date</columnTitle>
                    <nullable>false</nullable>
                    <propertyURI>http://cpas.labkey.com/Study#VisitDate</propertyURI>
                  </column>
                  <column columnName="tissue">
                    <datatype>varchar</datatype>
                    <columnTitle>Organ/Tissue</columnTitle>
                    <fk>
                      <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                      <fkDbSchema>lists</fkDbSchema>
                      <fkTable>snomed</fkTable>
                      <fkColumnName>code</fkColumnName>
                    </fk>
                  </column>
                  <column columnName="severity">
                    <datatype>varchar</datatype>
                    <columnTitle>Severity</columnTitle>
                    <fk>
                      <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                      <fkDbSchema>lists</fkDbSchema>
                      <fkTable>snomed</fkTable>
                      <fkColumnName>code</fkColumnName>
                    </fk>
                  </column>
                  <column columnName="Diagnosis">
                    <datatype>varchar</datatype>
                    <columnTitle>Diagnosis</columnTitle>
                    <fk>
                      <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                      <fkDbSchema>lists</fkDbSchema>
                      <fkTable>snomed</fkTable>
                      <fkColumnName>code</fkColumnName>
                    </fk>
                  </column>
                  <column columnName="remark">
                    <datatype>varchar</datatype>
                    <columnTitle>Remark</columnTitle>
                    <displayWidth>110</displayWidth>
                    <propertyURI>urn:ehr.labkey.org/#Remark</propertyURI>
                    <inputType>textarea</inputType>
                  </column>
                  <column columnName="seq1">
                    <datatype>integer</datatype>
                    <columnTitle>Diagnosis Number</columnTitle>
                  </column>
                  <column columnName="seq2">
                    <datatype>integer</datatype>
                    <columnTitle>Diagnosis Code Number</columnTitle>
                  </column>
                  <column columnName="tissue">
                    <datatype>varchar</datatype>
                    <!--<nullable>false</nullable>-->
                    <columnTitle>Process</columnTitle>
                    <fk>
                      <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                      <fkDbSchema>lists</fkDbSchema>
                      <fkTable>snomed</fkTable>
                      <fkColumnName>code</fkColumnName>
                    </fk>
                  </column>
                  <column columnName="duration">
                    <datatype>varchar</datatype>
                    <!--<nullable>false</nullable>-->
                    <columnTitle>Process</columnTitle>
                    <fk>
                      <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                      <fkDbSchema>lists</fkDbSchema>
                      <fkTable>snomed</fkTable>
                      <fkColumnName>code</fkColumnName>
                    </fk>
                  </column>
                  <column columnName="severity">
                    <datatype>varchar</datatype>
                    <!--<nullable>false</nullable>-->
                    <columnTitle>Process</columnTitle>
                    <fk>
                      <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                      <fkDbSchema>lists</fkDbSchema>
                      <fkTable>snomed</fkTable>
                      <fkColumnName>code</fkColumnName>
                    </fk>
                  </column>
                  <column columnName="process">
                    <datatype>varchar</datatype>
                    <!--<nullable>false</nullable>-->
                    <columnTitle>Process</columnTitle>
                    <fk>
                      <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                      <fkDbSchema>lists</fkDbSchema>
                      <fkTable>snomed</fkTable>
                      <fkColumnName>code</fkColumnName>
                    </fk>
                  </column>
                  <column columnName="distribution">
                    <datatype>varchar</datatype>
                    <!--<nullable>false</nullable>-->
                    <columnTitle>Process</columnTitle>
                    <fk>
                      <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                      <fkDbSchema>lists</fkDbSchema>
                      <fkTable>snomed</fkTable>
                      <fkColumnName>code</fkColumnName>
                    </fk>
                  </column>
                  <column columnName="description">
                    <datatype>varchar</datatype>
                    <columnTitle>Description</columnTitle>
                    <shownInInsertView>false</shownInInsertView>
                    <shownInUpdateView>false</shownInUpdateView>
                    <shownInDetailsView>false</shownInDetailsView>
                    <propertyURI>urn:ehr.labkey.org/#Description</propertyURI>
                    <isHidden>true</isHidden>
                    <inputType>textarea</inputType>
                  </column>
                  <column columnName="objectid">
                    <datatype>entityid</datatype>
                    <columnTitle>Key</columnTitle>
                    <shownInInsertView>false</shownInInsertView>
                    <shownInUpdateView>false</shownInUpdateView>
                    <shownInDetailsView>false</shownInDetailsView>
                    <isHidden>true</isHidden>
                    <!--<isKeyField>true</isKeyField>-->
                  </column>
                  <column columnName="parentid">
                    <datatype>varchar</datatype>
                    <columnTitle>Parent Id</columnTitle>
                    <propertyURI>urn:ehr.labkey.org/#ParentId</propertyURI>
                    <fk>
                      <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                      <fkDbSchema>study</fkDbSchema>
                      <fkTable>encounters</fkTable>
                      <fkColumnName>objectid</fkColumnName>
                    </fk>
                  </column>
                  <column columnName="ts">
                    <datatype>timestamp</datatype>
                    <columnTitle>Last Changed</columnTitle>
                    <!--<nullable>false</nullable>-->
                    <shownInInsertView>false</shownInInsertView>
                    <shownInUpdateView>false</shownInUpdateView>
                    <shownInDetailsView>false</shownInDetailsView>
                    <isHidden>true</isHidden>
                  </column>
                </columns>
                <tableTitle>Necropsy Diagnosis</tableTitle>
            </table>
        </tables>
    </metadata>
</query>