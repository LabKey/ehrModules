<customView xmlns="http://labkey.org/data/xml/queryCustomView">
  <columns>
      <column name="Id"/>
      <column name="date"/>
      <column name="parentId/caseno">
          <properties>
              <property name="columnTitle" value="Case No"/>
          </properties>
      </column>
      <column name="seq1"/>
      <column name="seq2"/>
      <column name="code"/>
      <column name="code/code">
          <properties>
              <property name="columnTitle" value="SNOMED Code"/>
          </properties>
      </column>
  </columns>
   <sorts>
       <sort column="id" />
       <sort column="date" />
       <sort column="seq1" />
       <sort column="seq2" />
   </sorts>
    <!--<filters>-->
        <!--<filter column="qcstate/publicdata" operator="eq" value="true"/>-->
    <!--</filters>-->
</customView>