<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="bacteriologyResults" tableDbType="TABLE">
                <tableUrl>/query/detailsQueryRow.view?schemaName=study&amp;query.queryName=Bacteriology Results&amp;lsid=${lsid}</tableUrl>
                <!--<insertUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Bacteriology Results&amp;keyField=lsid&amp;</insertUrl>-->
                <!--<updateUrl>/ehr/manageRecord.view?schemaName=study&amp;queryName=Bacteriology Results&amp;keyField=lsid&amp;key=${lsid}</updateUrl>-->
                <insertUrl />
                <importUrl />
                <updateUrl />
                <deleteUrl />
                <columns>
                    <column columnName="source">
                        <datatype>varchar</datatype>
                        <columnTitle>Source</columnTitle>
                        <fk>
                            <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>snomed</fkTable>
                            <fkColumnName>code</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="method">
                        <datatype>varchar</datatype>
                        <columnTitle>Method</columnTitle>
                        <fk>
                            <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>bacteriology_method</fkTable>
                            <fkColumnName>method</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="organism">
                        <datatype>varchar</datatype>
                        <columnTitle>Organism</columnTitle>
                        <fk>
                            <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>snomed</fkTable>
                            <fkColumnName>code</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="antibiotic">
                        <datatype>varchar</datatype>
                        <columnTitle>Antibiotic</columnTitle>
                        <fk>
                            <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>snomed</fkTable>
                            <fkColumnName>code</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="sensitivity">
                        <datatype>varchar</datatype>
                        <columnTitle>Sensitivity</columnTitle>
                        <fk>
                            <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>bacteriology_sensitivity</fkTable>
                            <fkColumnName>sensitivity</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="runId" wrappedColumnName="parentId">
                        <isHidden>false</isHidden>
                        <fk>
                          <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                          <fkDbSchema>study</fkDbSchema>
                          <fkTable>clinpathRunsParent</fkTable>
                          <fkColumnName>objectid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="qualresult">
                        <datatype>varchar</datatype>
                        <columnTitle>Qualitative Result</columnTitle>
                        <fk>
                            <fkFolderPath>/wnprc/ehr/</fkFolderPath>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>qualitative_results</fkTable>
                            <fkColumnName>result</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="parentId">
                        <!--<fk>-->
                          <!--<fkFolderPath>/wnprc/ehr/</fkFolderPath>-->
                          <!--<fkDbSchema>study</fkDbSchema>-->
                          <!--<fkTable>bacteriologyRuns</fkTable>-->
                          <!--<fkColumnName>objectid</fkColumnName>-->
                        <!--</fk>-->
                    </column>
                    <column columnName="date">
                        <formatString>yyyy-MM-dd</formatString>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>