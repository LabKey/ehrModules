<?xml version="1.0" encoding="UTF-8"?>
<ns:tables xmlns:ns="http://labkey.org/data/xml">
  <ns:table tableName="labanimals" tableDbType="TABLE">
    <ns:tableTitle>Lab Animals</ns:tableTitle>
    <ns:columns>
      <ns:column columnName="id">
        <ns:datatype>varchar</ns:datatype>
        <ns:scale>6</ns:scale>
      </ns:column>
      <ns:column columnName="sex">
        <ns:nullable>true</ns:nullable>
        <!--<ns:datatype>bpchar</ns:datatype>-->
        <ns:datatype>varchar</ns:datatype>
        <ns:scale>1</ns:scale>
      </ns:column>
      <ns:column columnName="dam">
        <ns:nullable>true</ns:nullable>
        <ns:datatype>varchar</ns:datatype>
        <ns:scale>6</ns:scale>
      </ns:column>
      <ns:column columnName="sire">
        <ns:nullable>true</ns:nullable>
        <ns:datatype>varchar</ns:datatype>
        <ns:scale>6</ns:scale>
      </ns:column>
      <ns:column columnName="birth">
        <!--<ns:datatype>date</ns:datatype>-->
        <ns:datatype>timestamp</ns:datatype>
        <ns:scale>13</ns:scale>
      </ns:column>
      <ns:column columnName="acquire">
        <!--<ns:datatype>date</ns:datatype>-->
        <ns:datatype>timestamp</ns:datatype>
        <ns:scale>13</ns:scale>
        <ns:propertyURI>http://cpas.labkey.com/Study#VisitDate</ns:propertyURI>
      </ns:column>
      <ns:column columnName="exit">
        <!--<ns:datatype>date</ns:datatype>-->
        <ns:datatype>timestamp</ns:datatype>
        <ns:scale>13</ns:scale>
      </ns:column>
      <ns:column columnName="acqcode">
        <ns:nullable>true</ns:nullable>
        <!--<ns:datatype>int4</ns:datatype>-->
        <ns:datatype>integer</ns:datatype>
        <ns:scale>10</ns:scale>
      </ns:column>
      <ns:column columnName="exitcode">
        <ns:nullable>true</ns:nullable>
        <!--<ns:datatype>int4</ns:datatype>-->
        <ns:datatype>integer</ns:datatype>
        <ns:scale>10</ns:scale>
      </ns:column>
    </ns:columns>
  </ns:table>
  <ns:table tableName="parares" tableDbType="TABLE">
    <ns:tableTitle>Parasitology</ns:tableTitle>
    <ns:columns>
      <ns:column columnName="key">
        <ns:datatype>integer</ns:datatype>
        <ns:scale>10</ns:scale>
        <ns:isKeyField>true</ns:isKeyField>
        <ns:isAutoInc>true</ns:isAutoInc>
      </ns:column>
      <ns:column columnName="id">
        <ns:datatype>varchar</ns:datatype>
        <ns:scale>6</ns:scale>
      </ns:column>
      <ns:column columnName="date">
        <!--<ns:datatype>date</ns:datatype>-->
        <ns:datatype>timestamp</ns:datatype>
        <ns:scale>13</ns:scale>
        <ns:propertyURI>http://cpas.labkey.com/Study#VisitDate</ns:propertyURI>
      </ns:column>
      <ns:column columnName="seq">
        <ns:nullable>true</ns:nullable>
        <!--<ns:datatype>int4</ns:datatype>-->
        <ns:datatype>integer</ns:datatype>
        <ns:scale>10</ns:scale>
      </ns:column>
      <ns:column columnName="code">
        <ns:nullable>true</ns:nullable>
        <ns:datatype>varchar</ns:datatype>
        <ns:scale>8</ns:scale>
        <ns:fk>
          <ns:fkDbSchema>lists</ns:fkDbSchema>
          <ns:fkTable>snomed</ns:fkTable>
          <ns:fkColumnName>code</ns:fkColumnName>
        </ns:fk>
      </ns:column>
    </ns:columns>
  </ns:table>
</ns:tables>
